{
  "name" : "GRIP",
  "description" : "Resilience analysis",
  "docker" : "slacgismo/gridlabd:develop",
  "git" : "https://github.com/openfido/resilience.git",
  "branch" : "main",
  "script" : "openfido.sh",
  "manual": {
    "gridlabd": "csv"
  },
  "gridlabd" : {
    "ANALYSIS" : {
      "description" : "Type of analysis to conduct",
      "choices" : "pole analysis, vegetation analysis, switch reconfiguration",
      "default" : "pole analysis",
      "input_type" : "enum"
    }, 
    "POLE_DATA" : {
      "prompt" : "Include pole data (csv)",
      "description" : "Pole data, csv",
      "default" : "",
      "input_type" : "upload"
    }
  }
} 
